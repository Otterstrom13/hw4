<% if @user != nil %>
<h1>Places</h1>

<ul>
  <% for place in @places %>
    <li><a href="/places/<%= place["id"] %>"><%= place["name"] %></a></li>
  <% end %>
</ul>

<p>

  <a href="/places/new">New place</a>
  
</p>

<% else %>

<a href="/login">Log In</a>

<a href="/users/new">Sign Up</a>

<% end %>

